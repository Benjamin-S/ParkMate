@model VehicleDTO

@{
    ViewData["Title"] = "Add a Vehicle";
}

<h1>Add A Vehicle</h1>

<form method="post" enctype="multipart/form-data" id="form">
    
    <div class="form-group">
        <label asp-for="Registration"></label>
        <input class="form-control" asp-for="Registration"/>
        <span asp-validation-for="Registration" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Make"></label>
        <select class="form-control" id="makeSelect" asp-for="Make">
            <option value="">--- Select an Option ---</option>
        </select>
        <span asp-validation-for="Make" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label  asp-for="Model"></label>
        <select class="form-control" id="modelSelect" asp-for="Model">
            <option value="">--- Select an Option ---</option>
        </select>
        <span asp-validation-for="Model" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label  asp-for="Color"></label>
        <select class="form-control" id="colourSelect" asp-for="Color">
            <option value="">--- Select an Option ---</option>
            <option>Aluminum</option>
            <option>Beige</option>
            <option>Black</option>
            <option>Blue</option>
            <option>Brown</option>
            <option>Bronze</option>
            <option>Claret</option>
            <option>Copper</option>
            <option>Cream</option>
            <option>Gold</option>
            <option>Gray</option>
            <option>Green</option>
            <option>Maroon</option>
            <option>Metallic</option>
            <option>Navy</option>
            <option>Orange</option>
            <option>Pink</option>
            <option>Purple</option>
            <option>Red</option>
            <option>Rose</option>
            <option>Rust</option>
            <option>Silver</option>
            <option>Tan</option>
            <option>Turquoise</option>
            <option>White</option>
            <option>Yellow</option>
        </select>
        <span asp-validation-for="Color" class="text-danger"></span>
    </div>
    
    <button type="submit" class="btn btn-primary mt-1">Submit</button>
</form>

