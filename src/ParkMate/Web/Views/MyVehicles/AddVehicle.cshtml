$.each(carData, function (key, value) {
    $('#makeSelect').append(
        $("<option>", { value: key, text: key })
    );
});

$("#makeSelect").change(function () {
    var textVal = $(this).val();
    var output = []

    for (var model in carData[textVal])
    {
        output.push('<option value ="' + carData[textVal][model] + '">' + carData[textVal][model] + '</option>');
    }

    // Set to array to reduce calls to the DOM
    $("#modelSelect").html(output.join(''));
});
    </script>
